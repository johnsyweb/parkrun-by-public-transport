{
  "name": "parkrun-by-public-transport",
  "version": "1.0.0",
  "description": "Find parkrun events near public transport stops",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write '**/*.{ts,tsx,json,css,html,md}'",
    "format:check": "prettier --check '**/*.{ts,tsx,json,css,html,md}'",
    "check": "pnpm typecheck && pnpm lint && pnpm format:check",
    "prepare": "husky"
  },
  "dependencies": {
    "leaflet": "^1.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/leaflet": "^1.9.21",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^5.4.21"
  }
}
